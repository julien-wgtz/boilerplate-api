{
  "name": "project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run copy-static",
    "postinstall": "npx prisma generate && npm run copy-static",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "npm-run-all --serial set-env start:dev",
    "start:dev": "dotenv -e config/.env.development -- nodemon",
    "start:debug": "nest start --debug --watch",
    "prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration": "dotenv -e config/.env.development -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e config/.env.development -- npx prisma generate",
    "dbpull:dev": "dotenv -e config/.env.development -- npx prisma db pull ",
    "dbpush:dev": "dotenv -e config/.env.development -- npx prisma db push ",
    "dbrestore:dev": "dotenv -e config/.env.development -- npx prisma migrate reset",
    "dbpush:prod": "dotenv -e config/.env.production -- npx prisma db push ",
    "dbrestore:prod": "dotenv -e config/.env.production -- npx prisma migrate reset",
    "copy-static": "cpx 'emails/static/**/*' dist/public",
    "set-env": "cross-env NODE_ENV=development",
    "email:dev": "email dev"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.709.0",
    "@aws-sdk/client-s3": "^3.709.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/websockets": "^10.4.7",
    "@prisma/client": "^5.21.1",
    "@react-email/components": "^0.0.25",
    "@react-email/render": "1.0.1",
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^9.0.1",
    "express-session": "^1.18.0",
    "i18next": "^23.15.1",
    "i18next-fs-backend": "^2.3.2",
    "lucide-react": "^0.460.0",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "prisma": "^5.21.1",
    "react-i18next": "^15.0.1",
    "reflect-metadata": "^0.1.13",
    "resend": "^4.0.0",
    "rxjs": "^7.8.1",
    "session": "^0.1.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.0",
    "react-email": "3.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
